{
    "@context": "http://cloudinformationmodel.org/",
    "@id": "ConversionPropertyGroup",
    "name": "Conversion Property Group",
    "@type": "PropertyGroup",
    "propertyConcepts": [
{
        "@id": "convertedDate",
        "@type": "Property",
        "description": "Date on which this Lead was converted.",
        "name": "Converted Date",
        "range": "DataType"
    },{
        "@id": "convertedToAccountContactId",
        "@type": "Property",
        "description": "Object reference ID that points to the AccountContact into which the Lead has been converted.",
        "name": "Converted To AccountContact",
        "range": "AccountContact"
    },{
        "@id": "convertedToAccountId",
        "@type": "Property",
        "description": "Object reference ID that points to the Account into which the Lead has been converted.",
        "name": "Converted To Account",
        "range": "Account"
    },{
        "@id": "isConverted",
        "@type": "Property",
        "description": "Indicates whether the Lead has been converted (true) or not (false)",
        "name": "Is Converted",
        "range": "DataType"
    }
    ]
}