{
    "@context": "http://cloudinformationmodel.org/",
    "@id": "BillingPropertyGroup",
    "name": "Billing Property Group",
    "@type": "PropertyGroup",
    "propertyConcepts": [
{
        "@id": "billingDayOfTheMonth",
        "@type": "Property",
        "description": "The day of the month that the customer should be invoiced",
        "name": "Billing Day of the Month",
        "range": "DataType"
    },{
        "@id": "billToAddressId",
        "@type": "Property",
        "description": "Reference to billing address",
        "name": "Bill To Address",
        "range": "ContactPointAddress"
    },{
        "@id": "billtoEmailId",
        "@type": "Property",
        "description": "Customer email address associated to the sales order",
        "name": "Bill to Email",
        "range": "ContactPointEmail"
    },{
        "@id": "billtoPhoneNumberId",
        "@type": "Property",
        "description": "Customer phone number associated with the sales order",
        "name": "Bill to Phone Number",
        "range": "ContactPointPhone"
    },{
        "@id": "balanceAmount",
        "@type": "Property",
        "description": "E.g. 100 points, 3 miles, 6 visits",
        "name": "Balance Amount",
        "range": "DataType"
    },{
        "@id": "balanceAmountCurrency",
        "@type": "Property",
        "description": "Currency for Balance Amount",
        "name": "Balance Amount Currency",
        "range": "Currency"
    },{
        "@id": "balanceAmountLimit",
        "@type": "Property",
        "description": "What is the max balance allowed before we may take action around usage. Sometimes referred to as credit limit",
        "name": "Balance Amount Limit",
        "range": "DataType"
    },{
        "@id": "balanceAmountLimitCurrency",
        "@type": "Property",
        "description": "Currency for Balance Amount Limit",
        "name": "Balance Amount Limit Currency",
        "range": "Currency"
    },{
        "@id": "balanceUnitOfMeasureCurrencyId",
        "@type": "Property",
        "description": "Currency used for accumulated money usage",
        "name": "Balance Unit Of Measure Currency",
        "range": "Currency"
    },{
        "@id": "balanceUnitOfMeasureId",
        "@type": "Property",
        "description": "E.g. money, points, miles, visits",
        "name": "Balance Unit Of Measure",
        "range": "AccountBalanceUnitOfMeasure"
    },{
        "@id": "billContactAddressId",
        "@type": "Property",
        "description": "Reference to the contact point billing address, if applicable",
        "name": "Bill Contact Address",
        "range": "ContactPointAddress"
    },{
        "@id": "billDeliveryMethodId",
        "@type": "Property",
        "description": "Electronic or Physical Mail delivery",
        "name": "Bill Delivery Method",
        "range": "BillDeliveryMethod"
    },{
        "@id": "lastActivityDate",
        "@type": "Property",
        "description": "Most recent of Due date of the most recent event logged or Due date of the most recently closed task",
        "name": "Last Activity Date",
        "range": "DataType"
    },{
        "@id": "ninetyDayBalanceAmount",
        "@type": "Property",
        "description": "Account Balance 90 days ago",
        "name": "Ninety Day Balance Amount",
        "range": "DataType"
    },{
        "@id": "ninetyDayBalanceAmountCurrency",
        "@type": "Property",
        "description": "Currency for 90 Day Aged Balance Amount",
        "name": "Ninety Day Balance Amount Currency",
        "range": "Currency"
    },{
        "@id": "parentAccountId",
        "@type": "Property",
        "description": "Id of the parent customer account (business role), if applicable",
        "name": "Parent Account",
        "range": "Account"
    },{
        "@id": "sixtyDayBalanceAmount",
        "@type": "Property",
        "description": "Account Balance 60 days ago",
        "name": "Sixty Day Balance Amount",
        "range": "DataType"
    },{
        "@id": "sixtyDayBalanceAmountCurrency",
        "@type": "Property",
        "description": "Currency for 60 Day Aged Balance Amount",
        "name": "Sixty Day Balance Amount Currency",
        "range": "Currency"
    },{
        "@id": "thirtyDayBalanceAmount",
        "@type": "Property",
        "description": "Account Balance 30 days ago",
        "name": "Thirty Day Balance Amount",
        "range": "DataType"
    },{
        "@id": "thirtyDayBalanceAmountCurrency",
        "@type": "Property",
        "description": "Currency for 30 Day Aged Balance Amount",
        "name": "Thirty Day Balance Amount Currency",
        "range": "Currency"
    }
    ]
}