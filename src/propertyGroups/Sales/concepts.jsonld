{
    "@context": "http://cloudinformationmodel.org/",
    "@id": "SalesPropertyGroup",
    "name": "Sales Property Group",
    "@type": "PropertyGroup",
    "propertyConcepts": [
{
        "@id": "isBackOrdered",
        "@type": "Property",
        "description": "If product is not in stock has it been backordered (planned to be in stock and cross all inventory locations)",
        "name": "Is Back Ordered",
        "range": "DataType"
    },{
        "@id": "isPreOrderable",
        "@type": "Property",
        "description": "If the product is not yet for sale can product be pre ordered",
        "name": "Is Pre Orderable",
        "range": "DataType"
    },{
        "@id": "isRainCheckAllowed",
        "@type": "Property",
        "description": "If product is not in stock can customer be promised get it later at todays price",
        "name": "Is Rain Check Allowed",
        "range": "DataType"
    },{
        "@id": "isReturnable",
        "@type": "Property",
        "description": "Can the product be returned or not",
        "name": "Is Returnable",
        "range": "DataType"
    },{
        "@id": "isSellable",
        "@type": "Property",
        "description": "Is this product intended to be sold or not (which may not be true if tracking external products or parts that make up the sellable product but need to be tracked for e.g. service purpose)",
        "name": "Is Sellable",
        "range": "DataType"
    },{
        "@id": "isSellableIndependently",
        "@type": "Property",
        "description": "Can product be sold by itself or only by being part of a bundle e.g. monogramming can ony be part of a bundle",
        "name": "Is Sellable Independently",
        "range": "DataType"
    },{
        "@id": "isSellableWithoutPrice",
        "@type": "Property",
        "description": "Can product be 'sold' without a price e.g. monogram added to a shirt for free",
        "name": "Is Sellable Without Price",
        "range": "DataType"
    },{
        "@id": "maximumOrderQuantityCount",
        "@type": "Property",
        "description": "Maximum quantity allowed for purchase e.g 5",
        "name": "Maximum Order Quantity Count",
        "range": "DataType"
    },{
        "@id": "minimumAdvertisementAmount",
        "@type": "Property",
        "description": "What is the lowest price allowed (normally by manufacturer) to use in an ad",
        "name": "Minimum Advertisement Amount",
        "range": "DataType"
    },{
        "@id": "minimumAdvertisementAmountCurrency",
        "@type": "Property",
        "description": "Currency for Minimum Advertisement Amount",
        "name": "Minimum Advertisement Amount Currency",
        "range": "Currency"
    },{
        "@id": "minimumAdvertisementAmountStartDate",
        "@type": "Property",
        "description": "What is the earliest date the lowest manufactureres prices can be stated in an ad",
        "name": "Minimum Advertisement Amount Start Date",
        "range": "DataType"
    },{
        "@id": "minimumOrderQuantityCount",
        "@type": "Property",
        "description": "Minimum quantity allowed for purchase e.g 1",
        "name": "Minimum Order Quantity Count",
        "range": "DataType"
    },{
        "@id": "mSRPAmount",
        "@type": "Property",
        "description": "The default price for the product i.e.Manufacturer Suggested Retail Price",
        "name": "MSRP Amount",
        "range": "DataType"
    },{
        "@id": "mSRPAmountCurrency",
        "@type": "Property",
        "description": "Currency for MSRP Amount",
        "name": "MSRP Amount Currency",
        "range": "Currency"
    },{
        "@id": "priceChargeTypeId",
        "@type": "Property",
        "description": "Is the item to be sold based on weight, by units or by usage",
        "name": "Price Charge Type Id",
        "range": "PriceChargeType"
    },{
        "@id": "primarySalesChannelId",
        "@type": "Property",
        "description": "The primary sales channel used to sell this product",
        "name": "Primary Sales Channel",
        "range": "SalesChannel"
    },{
        "@id": "productStatusId",
        "@type": "Property",
        "description": "Is the product In Design, In Test, Active or Inactive",
        "name": "Product Status",
        "range": "ProductStatus"
    },{
        "@id": "requiredDepositAmount",
        "@type": "Property",
        "description": "Amount that must be paid as deposit to be able to pickup or use the product",
        "name": "Required Deposit Amount",
        "range": "DataType"
    },{
        "@id": "requiredDepositAmountCurrency",
        "@type": "Property",
        "description": "Currency for Required Deposit Amount",
        "name": "Required Deposit Amount Currency",
        "range": "Currency"
    },{
        "@id": "requiredDepositPercentage",
        "@type": "Property",
        "description": "Percentage that must be paid up front as deposit to be able to pickup or use the product",
        "name": "Required Deposit Percentage",
        "range": "DataType"
    },{
        "@id": "requiresInvididualUnitPricing",
        "@type": "Property",
        "description": "Indicates high priced items or variable weight items need individual price",
        "name": "Requires Individual Unit Pricing",
        "range": "DataType"
    },{
        "@id": "externalRecordId",
        "@type": "Property",
        "description": "An ID used in an external system to uniquely identify a Delivery Method",
        "name": "External Record Id",
        "range": "DataType"
    },{
        "@id": "externalSourceId",
        "@type": "Property",
        "description": "The system in which the ExternalRecordId was assigned.",
        "name": "External Source Id",
        "range": "DataType"
    },{
        "@id": "isManuallyApplied",
        "@type": "Property",
        "description": "Was this Price Adjustment applied manually? An agent giving an adjustment that is not part of coupon, campaign, or promotion. False by default",
        "name": "Is Manually Applied",
        "range": "DataType"
    },{
        "@id": "priceAdjustmentMethodId",
        "@type": "Property",
        "description": "Defines how the adjustment will be calculated. Valid values used in Commerce Cloud are: percentage, fixed_price, amount, free, price_book_price, bonus, total_fixed_price, bonus_choice, percentage_off_options",
        "name": "Price Adjustment Method",
        "range": "PriceAdjustmentMethod"
    },{
        "@id": "reasonText",
        "@type": "Property",
        "description": "The reason description as to why the adjustment was given.",
        "name": "Reason",
        "range": "DataType"
    },{
        "@id": "salesOrderPriceAdjustmentTypeID",
        "@type": "Property",
        "description": "Defines which subtype an instance of the Price Adjustment Group is for: Sales Order, Sales Order Product, Fulfillment Product or Shipment Product.",
        "name": "Sales Order Price Adjustment Type Id",
        "range": "SalesOrderPriceAdjustmentType"
    },{
        "@id": "adjustedProductTaxAmount",
        "@type": "Property",
        "description": "Total Product Tax + Total Adjustment Tax Amount",
        "name": "Adjusted Product Tax Amount",
        "range": "DataType"
    },{
        "@id": "adjustedTotalProductAmount",
        "@type": "Property",
        "description": "Total Product Amount + Total Adjustment Amount",
        "name": "Adjusted Total Product Amount",
        "range": "DataType"
    },{
        "@id": "billToAccountId",
        "@type": "Property",
        "description": "The Account that will be billed for the Sales Order. Can only be updated when the orders StatusCode value is Draft.",
        "name": "Bill To Account",
        "range": "Account"
    },{
        "@id": "canBillNow",
        "@type": "Property",
        "description": "The customer is eligible to bill now if True",
        "name": "Can Bill Now",
        "range": "DataType"
    },{
        "@id": "confirmationRecipientEmailText",
        "@type": "Property",
        "description": "A list of email addresses to be messages for order confirmation. Not for large volume implementations",
        "name": "Confirmation Recipient Email",
        "range": "DataType"
    },{
        "@id": "grandTotalAmount",
        "@type": "Property",
        "description": "TotalProductAmount + TotalAdjustmentAmount + TotalTaxAmount",
        "name": "Grand Total Amount",
        "range": "DataType"
    },{
        "@id": "isAnonymous",
        "@type": "Property",
        "description": "True if the sales order was placed by an unregistered guest user",
        "name": "Is Anonymous",
        "range": "DataType"
    },{
        "@id": "isContracted",
        "@type": "Property",
        "description": "True if the sales order was placed under the terms of a contract",
        "name": "Is Contracted",
        "range": "DataType"
    },{
        "@id": "isHistoricalOnly",
        "@type": "Property",
        "description": "Flag the indicates whether or not the order is historical. If the order is Historical it a cannot be changed. Cancelations, invoices, shipping, refunds and other post sale activities cannot be driven from this order. This allows past orders to be imported",
        "name": "Is Historical Only",
        "range": "DataType"
    },{
        "@id": "isTaxExempt",
        "@type": "Property",
        "description": "True if taxes do not apply to this Sales Order",
        "name": "Is Tax Exempt",
        "range": "DataType"
    },{
        "@id": "orderNumber",
        "@type": "Property",
        "description": "Unique number assigned to this order and displayed to end users.",
        "name": "Order Number",
        "range": "DataType"
    },{
        "@id": "priceCalculationStatusMessageText",
        "@type": "Property",
        "description": "Text that describes the state of the price calculation process for the order",
        "name": "Price Calculation Status Message",
        "range": "DataType"
    },{
        "@id": "renewalUpliftRate",
        "@type": "Property",
        "description": "For renewal orders, the percentage of increase or decrease in order quantity",
        "name": "Renewal Uplift Rate",
        "range": "DataType"
    },{
        "@id": "salesOrderConfirmationStatusId",
        "@type": "Property",
        "description": "What stage of customer approval and finalization has been achieved.",
        "name": "Sales Order Confirmation Status",
        "range": "SalesOrderStatus"
    },{
        "@id": "sellerId",
        "@type": "Property",
        "description": "The party that plays the role of seller on the Service Order",
        "name": "Seller",
        "range": "Seller"
    },{
        "@id": "totalAdjustedDeliveryTaxAmount",
        "@type": "Property",
        "description": "? SalesOrderDeliveryGroup: TotalDeliveryTaxAmount",
        "name": "Total Adjusted Delivery Tax Amount",
        "range": "DataType"
    },{
        "@id": "totalBookingAmount",
        "@type": "Property",
        "description": "Total amount of Sales Order Product Booking amounts.",
        "name": "Total Booking Amount",
        "range": "DataType"
    },{
        "@id": "totalCancelledBillingAmount",
        "@type": "Property",
        "description": "Total of all canceled order products",
        "name": "Total Cancelled Billing Amount",
        "range": "DataType"
    },{
        "@id": "totalDeliveryAmount",
        "@type": "Property",
        "description": "A total of Product, Adjustment and Delivery fee amounts, but excluding Taxes.",
        "name": "Total Delivery Amount",
        "range": "DataType"
    },{
        "@id": "totalDeliveryFeeAmount",
        "@type": "Property",
        "description": "? SalesOrderDeliveryGroup: TotalDeliveryChargeAmount",
        "name": "Total Delivery Fee Amount",
        "range": "DataType"
    },{
        "@id": "totalPendingBillingAmount",
        "@type": "Property",
        "description": "Total pending for all order products.",
        "name": "Total Pending Billing Amount",
        "range": "DataType"
    },{
        "@id": "totalProductAmount",
        "@type": "Property",
        "description": "Roll up of all SalesOrderProduct.UnitPriceAmount instance amounts. Excludes order price adjustments (separate objects) and delivery costs.",
        "name": "Total Product Amount",
        "range": "DataType"
    },{
        "@id": "changeSalesOrderId",
        "@type": "Property",
        "description": "Id of the Change Order that revised an earlier Sales Order (the Original Sales Order).",
        "name": "Change Sales Order Id",
        "range": "SalesOrder"
    },{
        "@id": "changeSalesOrderProductId",
        "@type": "Property",
        "description": "Id of the Change Order Product that revised an earlier Sales Order (the Original Sales Order).",
        "name": "Change Sales Order Product Id",
        "range": "SalesOrderProduct"
    },{
        "@id": "relatedSalesOrderId",
        "@type": "Property",
        "description": "Id of the Sales Order that was later revised via the related Change order",
        "name": "Related Sales Order Id",
        "range": "SalesOrder"
    },{
        "@id": "relatedSalesOrderProductId",
        "@type": "Property",
        "description": "Id of a Sales Order Product that was later revised via the related Change order",
        "name": "Related Sales Order Product Id",
        "range": "SalesOrderProduct"
    },{
        "@id": "salesOrderChangeTypeCode",
        "@type": "Property",
        "description": "Codes that define reasons for changing a Sales Order, e.g. quantity change, new distributed adjustment, new line adjustmenmt, etc.",
        "name": "Sales Order Change Type Code",
        "range": "DataType"
    },{
        "@id": "accountContactId",
        "@type": "Property",
        "description": "The name of the person on the account who is the recipient of or contact for, the delivery",
        "name": "Delivery Contact",
        "range": "AccountContact"
    },{
        "@id": "contactPointAddressId",
        "@type": "Property",
        "description": "The ContactPointAddress for the shipping location. This entity stores contact info for the delivery contact, as well as the address.",
        "name": "Deliver to Address",
        "range": "ContactPointAddress"
    },{
        "@id": "deliveryInstructionsText",
        "@type": "Property",
        "description": "Instructions for the delivery",
        "name": "Delivery Instructions",
        "range": "DataType"
    },{
        "@id": "desiredDeliveryDate",
        "@type": "Property",
        "description": "The date the customer asked the delivery to occur",
        "name": "Desired Delivery Date",
        "range": "DataType"
    },{
        "@id": "giftMessageText",
        "@type": "Property",
        "description": "A message for the gift recipient",
        "name": "Gift Message",
        "range": "DataType"
    },{
        "@id": "grandTotalDeliveryAmount",
        "@type": "Property",
        "description": "TotalDeliveryAmount + TotalDeliveryTaxAmount -- equivalent to ? SalesOrderProduct(ProductId.ProductType == DeliveryCharge): TotalAmount",
        "name": "Grand Total Delivery Amount",
        "range": "DataType"
    },{
        "@id": "promisedDate",
        "@type": "Property",
        "description": "The date the seller promises to deliver the group of items",
        "name": "Promised Date",
        "range": "DataType"
    },{
        "@id": "salesOrderDeliveryGroupNumber",
        "@type": "Property",
        "description": "automatically-generated number that identifies the Sales Order Delivery Group",
        "name": "Sales Order Delivery Group Number",
        "range": "DataType"
    },{
        "@id": "totalDeliveryAdjustmentAmount",
        "@type": "Property",
        "description": "? OrderItem(Type == DeliveryCharge): TotalAdjustmentAmount",
        "name": "Total Delivery Adjustment Amount",
        "range": "DataType"
    },{
        "@id": "totalDeliveryAdjustmentTaxAmount",
        "@type": "Property",
        "description": "? OrderItem(Type == DeliveryCharge): TotalAdjustmentTaxAmount",
        "name": "Total Delivery Adjustment Tax Amount",
        "range": "DataType"
    },{
        "@id": "totalDeliveryAmountCurrencyId",
        "@type": "Property",
        "description": "Currency for Total Delivery Amount",
        "name": "Total Delivery Amount Currency",
        "range": "Currency"
    },{
        "@id": "totalDeliveryChargeAmount",
        "@type": "Property",
        "description": "? OrderItem(Type == DeliveryCharge): IF (TotalLineAmount != NULL, TotalLineAmount, UnitPrice * Quantity)",
        "name": "Total Delivery Charge Amount",
        "range": "DataType"
    },{
        "@id": "totalDeliveryChargeAmountCurrencyId",
        "@type": "Property",
        "description": "Currency for Total Delivery Charge Amount",
        "name": "Total Delivery Charge Amount Currency",
        "range": "Currency"
    },{
        "@id": "totalDeliveryChargeTaxAmount",
        "@type": "Property",
        "description": "? OrderItem(Type == DeliveryCharge): TotalProductTaxAmount",
        "name": "Total Delivery Charge Tax Amount",
        "range": "DataType"
    },{
        "@id": "totalDeliveryTaxAmount",
        "@type": "Property",
        "description": "TotalDeliveryLineTaxAmount + TotalDeliveryAdjustmentTaxAmount  -- equivalent to  ? OrderItem(Type == DeliveryCharge): TotalTaxAmount",
        "name": "Total Delivery Tax Amount",
        "range": "DataType"
    },{
        "@id": "appliedAmount",
        "@type": "Property",
        "description": "Total amount that has been applied.",
        "name": "Applied Amount",
        "range": "DataType"
    },{
        "@id": "authorizationAmount",
        "@type": "Property",
        "description": "Total amount that has been authorized.",
        "name": "Authorization Amount",
        "range": "DataType"
    },{
        "@id": "authorizationReversalAmount",
        "@type": "Property",
        "description": "Total amount that has been reversed from the authorized amount.",
        "name": "Authorization Reversal Amount",
        "range": "DataType"
    },{
        "@id": "availableToApplyBalanceAmount",
        "@type": "Property",
        "description": "Balance available for application of funds.",
        "name": "Available To Apply Balance Amount",
        "range": "DataType"
    },{
        "@id": "capturedAmount",
        "@type": "Property",
        "description": "Total amount that has been captured.",
        "name": "Captured Amount",
        "range": "DataType"
    },{
        "@id": "paymentAmount",
        "@type": "Property",
        "description": "The amount from the related Payment, which has been applied to the related Sales Order",
        "name": "Payment Amount",
        "range": "DataType"
    },{
        "@id": "unappliedAmount",
        "@type": "Property",
        "description": "Total amount that has been unapplied.",
        "name": "Unapplied Amount",
        "range": "DataType"
    },{
        "@id": "adjustedDeliveryTaxAmount",
        "@type": "Property",
        "description": "Rollup of Sales Order Product Tax rows, where the product is delivery fee, after any adjustments on delivery fees.",
        "name": "Adjusted Delivery Tax Amount",
        "range": "DataType"
    },{
        "@id": "commentText",
        "@type": "Property",
        "description": "A message displayed to the customer during order creation.",
        "name": "Comments",
        "range": "DataType"
    },{
        "@id": "deliveryTaxAmount",
        "@type": "Property",
        "description": "Rollup of Sales Order Product Tax rows, where the product is delivery fee, before any adjustments on delivery fees.",
        "name": "Delivery Tax Amount",
        "range": "DataType"
    },{
        "@id": "giftOrderMessageText",
        "@type": "Property",
        "description": "The message on the gift",
        "name": "Gift Order Message",
        "range": "DataType"
    },{
        "@id": "giftRecipientTelephoneNumber",
        "@type": "Property",
        "description": "Telephone number of the gift recipient",
        "name": "Gift Recipient Telephone Number",
        "range": "DataType"
    },{
        "@id": "isAutomaticallyRenewed",
        "@type": "Property",
        "description": "For a subscription-type product that can be automatically renewed, this setting indicates the order selection was for auto-renewal",
        "name": "Is Automatically Renewed",
        "range": "DataType"
    },{
        "@id": "isBonusProduct",
        "@type": "Property",
        "description": "Defines if the the sales order item a bonus product. Bonus products are usually free products add to an order. Being bonus has implecations on returns and adjustments.",
        "name": "Is Bonus Product",
        "range": "DataType"
    },{
        "@id": "isGift",
        "@type": "Property",
        "description": "Is this is a gift",
        "name": "Is Gift",
        "range": "DataType"
    },{
        "@id": "lineAdjustmentSubTotalAmount",
        "@type": "Property",
        "description": "Calculated as: UnitPriceTotalAmount - TotalLineAdjustmentAmount.     TotalDistributedAdjustmentAmount is not included.",
        "name": "Line Adjustment SubTotal Amount",
        "range": "DataType"
    },{
        "@id": "listPriceAmount",
        "@type": "Property",
        "description": "List price for one unit of the order product. Value is inherited from the associated PriceBookEntry upon order product creation.",
        "name": "List Price Amount",
        "range": "DataType"
    },{
        "@id": "orderAdjustmentSubTotalAmount",
        "@type": "Property",
        "description": "Calculated as: TotalUnitPriceAmount - TotalDistributedAdjustmentAmount.     TotalLineAdjustmentAmount is not included.",
        "name": "Order Adjustment SubTotal Amount",
        "range": "DataType"
    },{
        "@id": "orderManualAdjustmentSubTotalAmount",
        "@type": "Property",
        "description": "Calculated as: TotalUnitPriceAmount - TotalDistributedAdjustmentAmount.   TotalLineAdjustmentAmount is not included.",
        "name": "Order Manual Adjustment SubTotal Amount",
        "range": "DataType"
    },{
        "@id": "orderProductNumber",
        "@type": "Property",
        "description": "Automatically-generated number that identifies the order product.",
        "name": "Order Product Number",
        "range": "DataType"
    },{
        "@id": "originalOrderProductId",
        "@type": "Property",
        "description": "Required if is Reduction Order on the parent order is true. ID of the original order product being reduced.",
        "name": "Original Order Product",
        "range": "SalesOrderProduct"
    },{
        "@id": "recurringPriceAmount",
        "@type": "Property",
        "description": "A charge that will be incurred by the buyer on a repeating basis.",
        "name": "Recurring Price Amount",
        "range": "DataType"
    },{
        "@id": "requestedEndDate",
        "@type": "Property",
        "description": "When the product or service provisioning will end, from an order perspective",
        "name": "Requested End Date",
        "range": "DataType"
    },{
        "@id": "requestedStartDate",
        "@type": "Property",
        "description": "When the product or service provisioning will begin, from an order perspective",
        "name": "Requested Start Date",
        "range": "DataType"
    },{
        "@id": "salesOrderDeliveryGroupId",
        "@type": "Property",
        "description": "Sales Order Delivery Group",
        "name": "Sales Order Delivery Group",
        "range": "SalesOrderDeliveryGroup"
    },{
        "@id": "salesOrderId",
        "@type": "Property",
        "description": "Reference to the order that this order product is a child of",
        "name": "Sales Order",
        "range": "SalesOrder"
    },{
        "@id": "salesOrderProductAdjustedTaxAmount",
        "@type": "Property",
        "description": "TotalPriceAmount + TotalTaxAmount.    The adjusted product amount, with taxes and adjustment taxes applied.",
        "name": "Sales Order Product Adjusted Tax Amount",
        "range": "DataType"
    },{
        "@id": "salesOrderProductReasonId",
        "@type": "Property",
        "description": "The reason code for the Sales Order Product. For CPQ, would only be used to describe a cancellation reason",
        "name": "Sales Order Product Reason",
        "range": "SalesOrderProductReason"
    },{
        "@id": "salesOrderProductStatusId",
        "@type": "Property",
        "description": "Status of the product within the order. Values may include Placed and Activated",
        "name": "Sales Order Product Status",
        "range": "SalesOrderProductStatus"
    },{
        "@id": "segmentIndexId",
        "@type": "Property",
        "description": "Used with Segment Indexes for pricing subscriptions. A Segment Index is unique to a customer's purchase of a product that uses segment pricing, and is used to group the segmements together.",
        "name": "Segment Index",
        "range": "DataType"
    },{
        "@id": "sellerAccountId",
        "@type": "Property",
        "description": "Lookup to the Seller Account assigne to this order product. This will normally be used only in marketplace configurations, where the ecommerce site provides a customer with access to products that are sold by multiple other entities.",
        "name": "Seller Account",
        "range": "Account"
    },{
        "@id": "totalAdjustmentAmount",
        "@type": "Property",
        "description": "TotalLineAdjustmentAmount + TotalDistributedAdjustmentAmount +   Total Manual Adjustment Amount. A positive value which when subtracted from (quantity*ListPriceAmount) determines TotalPriceAmount",
        "name": "Total Adjustment Amount",
        "range": "DataType"
    },{
        "@id": "totalAdjustmentTaxAmount",
        "@type": "Property",
        "description": "The sum of all taxes computed on line-level adjustments (excludes distributed/order-level adjustments).    ? SalesOrderProductPriceAdjustment.SalesOrderProductPriceAdjustmentTax: TaxAmount, where PriceAdjustmentGroup.PriceAdjustmentTypeID = SalesOrderProd",
        "name": "Total Adjustment Tax Amount",
        "range": "DataType"
    },{
        "@id": "totalDistributedAdjustmentAmount",
        "@type": "Property",
        "description": "? SalesOrderProductPriceAdjustment(SalesOrderProductPriceAdjustmentType == Distributed): AdjustmentAmount.  Excludes SalesOrderProduct and Manual adjustments.",
        "name": "Total Distributed Adjustment Amount",
        "range": "DataType"
    },{
        "@id": "totalDistributedAdjustmentTaxAmount",
        "@type": "Property",
        "description": "The sum of all taxes computed on order-level adjustments that are prorated to SalesOrderProduct (excludes taxes on SalesOrderProduct-level adjustments).    ? SalesOrderProductPriceAdjustment: AdjustmentTax, where PriceAdjustmentGroup.PriceAdjustmentTypeID",
        "name": "Total Distributed Adjustment Tax Amount",
        "range": "DataType"
    },{
        "@id": "totalDistributedTaxAmount",
        "@type": "Property",
        "description": "Total line level taxes on distributed adjustments",
        "name": "Total Distributed Tax Amount",
        "range": "DataType"
    },{
        "@id": "totalLineAdjustmentAmount",
        "@type": "Property",
        "description": "? SalesOrderProductPriceAdjustment(SalesOrderProductPriceAdjustmentType == SalesOrderProduct): AdjustmentAmount.  Excludes Distributed and Manual adjustments.",
        "name": "Total Line Adjustment Amount",
        "range": "DataType"
    },{
        "@id": "totalLineAmount",
        "@type": "Property",
        "description": "Provides a way to override the standard calculation of the SalesOrderProduct's TotalPriceAmount.",
        "name": "Total Line Amount",
