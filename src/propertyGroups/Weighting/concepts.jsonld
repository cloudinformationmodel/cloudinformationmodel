{
    "@context": "http://cloudinformationmodel.org/",
    "@id": "WeightingPropertyGroup",
    "name": "Weighting Property Group",
    "@type": "PropertyGroup",
    "propertyConcepts": [
{
        "@id": "drainedWeight",
        "@type": "Property",
        "description": "What is the weigth of the product excluding packaging after drainage",
        "name": "Drained Weight",
        "range": "DataType"
    },{
        "@id": "grossWeight",
        "@type": "Property",
        "description": "What is the weight of the product in its original packaging",
        "name": "Gross Weight",
        "range": "DataType"
    },{
        "@id": "netWeight",
        "@type": "Property",
        "description": "What is the weight of the product excluding packaging",
        "name": "Net Weight",
        "range": "DataType"
    },{
        "@id": "tareWeight",
        "@type": "Property",
        "description": "What is the weigth of the product packaging without product content",
        "name": "Tare Weight",
        "range": "DataType"
    },{
        "@id": "weightUOMId",
        "@type": "Property",
        "description": "What is the unit of measure for stating the weight of the product",
        "name": "Weight UOM",
        "range": "WeightUnitOfMeasure"
    }
    ]
}