{
    "@context": "http://cloudinformationmodel.org/",
    "@id": "UsePropertyGroup",
    "name": "Use Property Group",
    "@type": "PropertyGroup",
    "propertyConcepts": [
{
        "@id": "fromDate",
        "@type": "Property",
        "description": "Active to date",
        "name": "From Date",
        "range": "DataType"
    },{
        "@id": "isDefaultOption",
        "@type": "Property",
        "description": "Is this the default option for a product e.g. phone case PC1 is default for phone P2",
        "name": "Is Default Option",
        "range": "DataType"
    },{
        "@id": "productRelationshipTypeId",
        "@type": "Property",
        "description": "What is the relation between product 1 and 2 e.g. consist of/component of",
        "name": "Product Relationship Type",
        "range": "ProductRelationshipType"
    },{
        "@id": "toDate",
        "@type": "Property",
        "description": "Active to date",
        "name": "To Date",
        "range": "DataType"
    },{
        "@id": "parentCategoryId",
        "@type": "Property",
