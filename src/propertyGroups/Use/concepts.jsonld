        "name": "Primary Flag",
        "range": "DataType"
    },{
        "@id": "isFaxCapable",
        "@type": "Property",
        "description": "Is the phone number able to receive fax",
        "name": "Is Fax Capable",
        "range": "DataType"
    },{
        "@id": "isSMSCapable",
        "@type": "Property",
        "description": "Is the phone capable to send and receive SMS",
        "name": "Is SMS Capable",
        "range": "DataType"
    },{
        "@id": "primaryPhoneTypeId",
        "@type": "Property",
        "description": "Is it primarily a 'Home', 'Business', 'Fax', 'Mobile' or 'Other' phone",
        "name": "Primary Phone Type",
        "range": "PrimaryPhoneType"
    },{
        "@id": "followersCount",
        "@type": "Property",
        "description": "How many followers does the handle have",
        "name": "Followers Count",
        "range": "DataType"
    },{
        "@id": "socialFanStatusId",
        "@type": "Property",
        "description": "Does the social persona like our products e.g. Influencer, Fan, Detractor, Neutral",
        "name": "Social Fan Status",
        "range": "SocialFanStatus"
    }
    ]
}