        "name": "Parent Category",
        "range": "ProductCategory"
    },{
        "@id": "activeFromDateTime",
        "@type": "Property",
        "description": "From what date is the product active within the category",
        "name": "Active From Date Time",
        "range": "DataType"
    },{
        "@id": "activeToDateTime",
        "@type": "Property",
        "description": "To what date is the product active within the category",
        "name": "Active To Date Time",
        "range": "DataType"
    },{
        "@id": "activeFromDate",
        "@type": "Property",
        "description": "When contact point start to be active",
        "name": "Active From Date",
        "range": "DataType"
    },{
        "@id": "activeToDate",
        "@type": "Property",
        "description": "When contact point stated to no longer be active",
        "name": "Active To Date",
        "range": "DataType"
    },{
        "@id": "bestTimeToContactEndTime",
        "@type": "Property",
        "description": "Best time during day to contact via this contact point",
        "name": "Best Time To Contact End Time",
        "range": "DataType"
    },{
        "@id": "bestTimeToContactStartTime",
        "@type": "Property",
        "description": "Best time during day to contact via this contact point",
        "name": "Best Time To Contact Start Time",
        "range": "DataType"
    },{
        "@id": "forBusinessUse",
        "@type": "Property",
        "description": "Is the phone used for business purpose",
        "name": "For Business Use",
        "range": "DataType"
    },{
        "@id": "forPersonalUse",
        "@type": "Property",
        "description": "Is the phone used for personal purpose",
        "name": "For Personal Use",
        "range": "DataType"
    },{
        "@id": "isUndeliverable",
        "@type": "Property",
        "description": "Used to manage a ContactPoint when messages are bouncing and the address undeliverable",
        "name": "Is Undeliverable",
        "range": "DataType"
    },{
        "@id": "primaryFlag",
        "@type": "Property",
