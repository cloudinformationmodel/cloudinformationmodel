{
    "@context": "http://cloudinformationmodel.org/",
    "@id": "DefinePropertyGroup",
    "name": "Define Property Group",
    "@type": "PropertyGroup",
    "propertyConcepts": [
{
        "@id": "partyId",
        "@type": "Property",
        "description": "What is the first party this relation involves",
        "name": "Party",
        "range": "Party"
    },{
        "@id": "partyRelationshipTypeId",
        "@type": "Property",
        "description": "Type of affilation",
        "name": "Party Relationship Type",
        "range": "PartyRelationshipType"
    },{
        "@id": "relatedFromDate",
        "@type": "Property",
        "description": "Date when affiliation starts",
        "name": "Related From Date",
        "range": "DataType"
    },{
        "@id": "relatedPartyId",
        "@type": "Property",
        "description": "What is the secondary party this relation involves",
        "name": "Related Party",
        "range": "Party"
    },{
        "@id": "relatedToDate",
        "@type": "Property",
        "description": "Date when affiliation ends",
        "name": "Related To Date",
        "range": "DataType"
    },{
        "@id": "isBidirectional",
        "@type": "Property",
        "description": "If the relationship is not restricted from role 1 to role 2 then set this to true",
        "name": "Is Bidirectional",
        "range": "DataType"
    },{
        "@id": "partyRoleId",
        "@type": "Property",
        "description": "What is the first party role this relation involves",
        "name": "Party Role",
        "range": "PartyRelationshipRole"
    },{
        "@id": "relatedPartyRoleId",
        "@type": "Property",
        "description": "What is the secondary party role this relation involves",
        "name": "Related Party Role",
        "range": "PartyRelationshipRole"
    },{
        "@id": "accountId",
        "@type": "Property",
        "description": "Reference to the Account the Partner have access to",
        "name": "Account",
        "range": "Account"
    },{
        "@id": "partnerAccountId",
        "@type": "Property",
        "description": "Reference to Partner type of Account",
        "name": "Partner Account",
        "range": "Account"
    }
    ]
}