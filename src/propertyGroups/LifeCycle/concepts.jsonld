{
    "@context": "http://cloudinformationmodel.org/",
    "@id": "LifeCyclePropertyGroup",
    "name": "Life Cycle Property Group",
    "@type": "PropertyGroup",
    "propertyConcepts": [
{
        "@id": "leadSourceId",
        "@type": "Property",
        "description": "Source from which the lead was obtained.",
        "name": "Lead Source",
        "range": "LeadSource"
    },{
        "@id": "leadStatusId",
        "@type": "Property",
        "description": "Status for this converted lead e.g. open, worked, closed",
        "name": "Lead Status",
        "range": "LeadStatus"
    },{
        "@id": "customerStatusId",
        "@type": "Property",
        "description": "What status customer is in e.g. prospect, active, inactive, nonrecoverable, hold, expired",
        "name": "Customer Status",
        "range": "CustomerStatus"
    },{
        "@id": "prospectRatingId",
        "@type": "Property",
        "description": "The customers prospect rating, for example Hot, Warm, or Cold.",
        "name": "Prospect Rating",
        "range": "ProspectRating"
    },{
        "@id": "holdStatusReasonId",
        "@type": "Property",
        "description": "Reference to the reason why the account is on hold status",
        "name": "Hold Status Reason",
        "range": "AccountHoldStatusReason"
    }
    ]
}