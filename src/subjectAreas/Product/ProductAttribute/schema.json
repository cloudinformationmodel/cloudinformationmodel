{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "oneOf": [
    {
      "$ref": "#/definitions/AttributeSetTranslation"
    },
    {
      "$ref": "#/definitions/AttributeTranslation"
    },
    {
      "$ref": "#/definitions/AttributeValue"
    },
    {
      "$ref": "#/definitions/AttributeValueTranslation"
    },
    {
      "$ref": "#/definitions/DeviceUserSession"
    },
    {
      "$ref": "#/definitions/PriceBookEntry"
    },
    {
      "$ref": "#/definitions/ProductAttributeSet"
    },
    {
      "$ref": "#/definitions/ProductAttributeValue"
    },
    {
      "$ref": "#/definitions/ProductCollateral"
    },
    {
      "$ref": "#/definitions/ProductValidityTimePeriodUOM"
    }
  ],
  "definitions": {
    "AttributeValue": {
      "type": "object",
      "required": [
        "attributeValue",
        "id"
      ],
      "properties": {
        "attributeValue": {
          "type": "string"
        },
        "id": {
          "type": "string"
        }
      }
    },
    "DeviceUserSession": {
      "type": "object",
      "required": [
        "id",
        "name"
      ],
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        }
      }
    },
    "ProductCollateral": {
      "type": "object",
      "required": [
        "product",
        "id",
        "uRL"
      ],
      "properties": {
        "product": {
          "type": "array",
          "items": {
            "$ref": "../Product/schema.json#/definitions/Product"
          }
        },
        "id": {
          "type": "string"
        },
        "uRL": {
          "type": "string"
        }
      }
    },
    "AttributeTranslation": {
      "type": "object",
      "required": [
        "id",
        "translatedName"
      ],
      "properties": {
        "id": {
          "type": "string"
        },
        "translatedName": {
          "type": "string"
        }
      }
    },
    "ProductValidityTimePeriodUOM": {
      "type": "object",
      "required": [
        "id",
        "name",
        "pluralName"
      ],
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "pluralName": {
          "type": "string"
        }
      }
    },
    "ProductAttributeSet": {
      "type": "object",
      "required": [
        "product",
        "id",
        "attributeSet"
      ],
      "properties": {
        "product": {
          "type": "array",
          "items": {
            "$ref": "../Product/schema.json#/definitions/Product"
          }
        },
        "id": {
          "type": "string"
        },
        "attributeSet": {
          "type": "string"
        }
      }
    },
    "ProductAttributeValue": {
      "type": "object",
      "required": [
        "product",
        "id",
        "name",
        "attributeSet"
      ],
      "properties": {
        "product": {
          "type": "array",
          "items": {
            "$ref": "../Product/schema.json#/definitions/Product"
          }
        },
        "attributeValue": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/AttributeValue"
          }
        },
        "id": {
          "type": "string"
        },
        "customAttributeValue": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "attributeSet": {
          "type": "string"
        }
      }
    },
    "AttributeSetTranslation": {
      "type": "object",
      "required": [
        "id",
        "translatedName",
        "attributeSet"
      ],
      "properties": {
        "id": {
          "type": "string"
        },
        "translatedName": {
          "type": "string"
        },
        "attributeSet": {
          "type": "string"
        }
      }
    },
    "PriceBookEntry": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "minRequiredQuantity": {
          "type": "integer"
        },
        "unitListPrice": {
          "type": "integer"
        },
        "product": {
          "type": "array",
          "items": {
            "$ref": "../Product/schema.json#/definitions/Product"
          }
        },
        "minRequiredOrderValue": {
          "type": "integer"
        },
        "isActive": {
          "type": "boolean"
        },
        "id": {
          "type": "string"
        },
        "maxRequiredOrderValue": {
          "type": "integer"
        },
        "useStandardPrice": {
          "type": "boolean"
        },
        "servicePeriodCount": {
          "type": "integer"
        }
      }
    },
    "AttributeValueTranslation": {
      "type": "object",
      "required": [
        "translatedAttributeValue",
        "attributeValue",
        "id"
      ],
      "properties": {
        "translatedAttributeValue": {
          "type": "string"
        },
        "attributeValue": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/AttributeValue"
          }
        },
        "id": {
          "type": "string"
        },
        "translatedAttributeValueDescription": {
          "type": "string"
        }
      }
    }
  }
}
